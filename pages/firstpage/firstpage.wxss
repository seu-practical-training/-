page {
  background-color: #fff;
  font-family: "Helvetica Neue",
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 "\9ED1\4F53",
                 Arial,
                 sans-serif;
  font-size: 32rpx;
}

.navigator-hover {
  background: none;
}

.search {
  padding: 20rpx;
  background-color:rgb(249,242,230);
}

.search-text {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 50rpx;
  color: #aaa;
  text-align: center;
  border: 1px solid #ddd;
  border-radius: 20rpx;
}

.search-text icon {
  margin-right: 10rpx;
}

swiper {
  height: 421.5rpx;
}

swiper-item image {
  width: 100%;
  height: 100%;
}

.selected{
 background-color:rgb(249,242,230);
}

.select-title, .newest-title {
  width: 100%;
  padding: 30rpx 0;
  height: 40rpx;
  color: #ab956d;
  text-align: center;
}

.select-top-small {
  width: 375rpx;
  height: 375rpx;
  float: left;
}

.select-bottom, .select-top {
  width: 100%;
  height: 375rpx;
}

.select-top-small image, .select-bottom image, .select-top-small navigator,
.select-bottom navigator {
  display: block;
  width: 100%;
  height: 100%;
}

.newest{
background-color:rgb(249,242,230)
}

/* .newest-box {
  padding: 0rpx 30rpx;
} */


.newest-box .newest-list:nth-child(2n) {
  margin-right: 0;
}

.newest-box .newest-list image {
  width: 300rpx;
  height: 300rpx;
  margin: 10rpx 0 0rpx;
  border-radius: 30px;
}

.newest-box .newest-list .newest-text {
  font-size: 28rpx;
  color: #ab956d;
  /* display: inline; */
  /* margin: 0px 20rpx; */
  /* overflow: hidden; /* 超出一行文字自动隐藏*/
  /* text-overflow: ellipsis; 文字隐藏后添加省略号 */
  /* white-space: nowrap; 强制不换行 */ 
    /* 多行显示、并加省略号 */
  display: -webkit-box;
  line-height: 33rpx;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
}

.goTop{
  position: fixed;
  bottom: 0rpx;
  right: 5rpx;
  border-radius: 50%;
  width: 250rpx;
  height: 220rpx;
  margin: 10rpx 0 10rpx;
}

.newest-box .newest-list {
  display: inline-block;
  width: 360rpx;
  height: 400rpx;
  margin: 20rpx 20rpx 0rpx 0rpx; 
  border-radius: 30px;
  text-align: center;
  border: 1px solid rgb(249,242,230);
  background-size:100% 100%;
  background-repeat:no-repeat;
  background-image:url('https://tva1.sinaimg.cn/large/006y8mN6ly1g6on0aoa0zj30u00u010l.jpg');
  }